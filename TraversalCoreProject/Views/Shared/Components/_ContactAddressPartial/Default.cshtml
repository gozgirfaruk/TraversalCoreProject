@model List<Address>

@foreach(var item in Model)
{
    <div class="partners">
    <div class="cont-details">
        <h5>Get in touch</h5>
        <p class="mt-3 mb-4">
           @item.Description
        </p>
    </div>
    <div class="hours">
        <h6 class="mt-4">E-Posta:</h6>
        <p>
            <a>
               @item.Mail
            </a>
        </p>
        <h6 class="mt-4">Bizi Ziyaret Edin:</h6>
        <p> @item.Location</p>
        <h6 class="mt-4">İletişim:</h6>
        <p class="margin-top"><a>@item.Phone</a></p>
    </div>
</div>
}

